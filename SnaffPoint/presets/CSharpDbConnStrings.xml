<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>CSharpDbConnStrings</Name>
  <Request>
    <QueryText>NEAR("data source", "password", n=30)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("aspx","ashx","asmx","asp","cshtml","cs","ascx","config")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>